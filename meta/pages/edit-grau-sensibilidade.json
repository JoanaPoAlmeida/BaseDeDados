{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Limite Maximo",
                              "component": "limite_maximo",
                              "name": "limite_maximoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "double",
                              "type": "numeric",
                              "name": "limite_maximo",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${grausensibilidade.limite_maximo}"
                            },
                            {
                              "type": "required-validator",
                              "name": "limite_maximoRequiredValidator",
                              "component": "limite_maximo",
                              "text": "limite_maximo is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Limite Minimo",
                              "component": "limite_minimo",
                              "name": "limite_minimoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "double",
                              "type": "numeric",
                              "name": "limite_minimo",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${grausensibilidade.limite_minimo}"
                            },
                            {
                              "type": "required-validator",
                              "name": "limite_minimoRequiredValidator",
                              "component": "limite_minimo",
                              "text": "limite_minimo is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update GrauSensibilidade",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "radnetBD.updateGrauSensibilidade",
                        "parameters": [
                          {
                            "name": "id_Grau",
                            "value": "${parameters.id_Grau}"
                          },
                          {
                            "name": "GrauSensibilidade",
                            "value": "${grausensibilidade}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${grausensibilidade}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Radnet.Models.RadnetBd.GrauSensibilidade",
                  "data": "${grausensibilidade}",
                  "visible": "${grausensibilidade != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "radnetBD.getGrauSensibilidadeByid_Grau",
        "parameters": [
          {
            "name": "id_Grau",
            "value": "${parameters.id_Grau}"
          }
        ],
        "then": [
          {
            "name": "grausensibilidade",
            "type": "set",
            "value": "${result}",
            "valueType": "Radnet.Models.RadnetBd.GrauSensibilidade"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Grau Sensibilidade",
  "layout": "Main"
}