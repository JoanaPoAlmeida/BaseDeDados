{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Estacao"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-estacao",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getEstacaosResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "id_estacao",
                            "value": "${event.id_estacao}"
                          }
                        ],
                        "path": "edit-estacao",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "id_estacao",
                      "title": "Id Estacao"
                    },
                    {
                      "property": "tipo_estacao",
                      "title": "Tipo Estacao"
                    },
                    {
                      "property": "freq_leitura",
                      "title": "Freq Leitura"
                    },
                    {
                      "property": "localizacao",
                      "title": "Localizacao"
                    },
                    {
                      "property": "data_instalacao",
                      "formatString": "{0:d}",
                      "title": "Data Instalacao"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete Estacao",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "radnetBD.deleteEstacao",
                                "parameters": [
                                  {
                                    "name": "id_estacao",
                                    "value": "${data.id_estacao}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "datagrid",
                  "itemType": "Radnet.Models.RadnetBd.Estacao"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "radnetBD.getEstacaos",
        "parameters": [
          {
            "name": "$expand",
            "value": "Sensors"
          }
        ],
        "then": [
          {
            "name": "getEstacaosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Radnet.Models.RadnetBd.Estacao>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Estacao",
  "layout": "Main"
}