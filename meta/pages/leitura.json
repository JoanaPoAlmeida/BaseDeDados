{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Leitura"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getLeiturasResult}",
                  "events": {
                    "loadData": []
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "id_leitura",
                      "title": "Id Leitura"
                    },
                    {
                      "property": "data_leitura",
                      "formatString": "{0:d}",
                      "title": "Data Leitura"
                    },
                    {
                      "property": "hora_leitura",
                      "title": "Hora Leitura"
                    },
                    {
                      "property": "valor_leitura",
                      "title": "Valor Leitura"
                    },
                    {
                      "property": "id_estacao",
                      "title": "Id Estacao"
                    }
                  ],
                  "type": "datagrid",
                  "itemType": "Radnet.Models.RadnetBd.Leitura"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "radnetBD.getLeituras",
        "parameters": [],
        "then": [
          {
            "name": "getLeiturasResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Radnet.Models.RadnetBd.Leitura>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Leitura",
  "layout": "Main"
}